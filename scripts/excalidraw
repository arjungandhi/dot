#!/usr/bin/bash
if [[ -n $COMP_LINE ]]; then
    prefix=$(echo "$COMP_LINE" | cut -d " " -f 2)
    # echo valid completions after this point
    # iterate through dirs and search
       find *.excalidraw -maxdepth 1 -type f -printf "%f" | grep "$prefix"
    exit
fi


